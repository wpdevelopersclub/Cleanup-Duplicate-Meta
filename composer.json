{
    "name" 			: "hellofromtonya/cleanup-duplicate-meta",
    "description" 	: "Deletes duplicate entries in the wp_postmeta and wp_usermeta database tables",
    "type"          : "project",
    "keywords" 		: ["wordpress", "wp", "plugin"],
    "homepage" 		: "https://github.com/hellofromtonya/Cleanup-Duplicate-Meta",
    "license" 		: "GPL-2.0+",
    "authors": [
        {
            "name" 	: "Tonya",
            "email" : "hellofromtonya@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "autoload" : {
        "classmap" : [
            "class-plugin.php",

            "lib/models/i-model.php",
            "lib/models/class-model.php",
            "lib/models/class-postmeta.php",
            "lib/models/class-usermeta.php"
        ],
        "files": []
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "vendor-dir": "assets/vendor"
    },
    "minimum-stability": "dev"
}